	<queryString>
		<![CDATA[select
     coc.objectnumber as accession_id,
     hcoc.name as collectionobject_csid,
     tc.displayname as scientific_name
from collectionobjects_common coc
left outer join misc mcoc ON (coc.id = mcoc.id)
left outer join hierarchy htig on (
        coc.id = htig.parentid
        and htig.primarytype = 'taxonomicIdentGroup'
        and htig.pos = 0)
left outer join taxonomicidentgroup tig on (htig.id = tig.id)
left outer join taxon_common tc on (tig.taxon = tc.refname)
left outer join taxon_ucjeps tu on (tc.id = tu.id)
left outer join hierarchy hcoc on (coc.id = hcoc.id)
left outer join relations_common rc on (
        hcoc.name = rc.subjectcsid
        and rc.subjectdocumenttype = 'CollectionObject'
        and rc.objectdocumenttype = 'Media')
where tu.taxonmajorgroup = 'Bryophytes'
and rc.id is null
and mcoc.lifecyclestate != 'deleted'
and coc.objectnumber !~* 'DHN'
order by coc.objectnumber]]>
	</queryString>
	<field name="accession_id" class="java.lang.String"/>
	<field name="collectionobject_csid" class="java.lang.String"/>
	<field name="scientific_name" class="java.lang.String"/>
