	<queryString>
		<![CDATA[select count(*) as dup_count, x.title as film_title, x.doc_id as film_ids
from (
select
        replace(wc.shortidentifier, 'pfafilm', '') as doc_id,
        wtg.termdisplayname as title
from works_common wc
inner join hierarchy hwtg on (wc.id = hwtg.parentid and primarytype = 'workTermGroup')
inner join worktermgroup wtg on (hwtg.id = wtg.id)
) x
group by x.doc_id, x.title
having count(*) > 1
UNION
select y.c as dup_count, y.title as film_title, concat_docids(y.title) as film_ids
from (
select count(*) as c, x.title from (
        select distinct
                replace(wc.shortidentifier, 'pfafilm', '') as doc_id,
                wtg.termdisplayname as title
        from works_common wc
        inner join hierarchy hwtg on (wc.id = hwtg.parentid and primarytype = 'workTermGroup')
        inner join worktermgroup wtg on (hwtg.id = wtg.id)
        ) x
        group by x.title
        having count(*) > 1
) y
order by film_title]]>
	</queryString>
	<field name="dup_count" class="java.lang.Long"/>
	<field name="film_title" class="java.lang.String"/>
	<field name="film_ids" class="java.lang.String"/>
