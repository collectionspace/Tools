			</textField>
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement stretchType="RelativeToBandHeight" x="132" y="105" width="388" height="18" isPrintWhenDetailOverflows="true"/>
				<textElement/>
