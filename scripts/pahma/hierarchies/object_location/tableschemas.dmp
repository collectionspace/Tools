-- Name: nx_log_hierarchy_read_acl_modified(); Type: FUNCTION; Schema: public; Owner: nuxeo
--

CREATE FUNCTION nx_log_hierarchy_read_acl_modified() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
-- Trigger to update the aclr tables
BEGIN
  IF (NEW.acl_id IS NOT NULL) THEN
    INSERT INTO aclr
      SELECT md5(acl), acl FROM (SELECT nx_get_read_acl(NEW.id) AS acl) AS input
      WHERE NOT EXISTS (SELECT 1 FROM aclr AS r WHERE r.acl_id = NEW.acl_id);
  END IF;
  RETURN NEW;
END $$;


