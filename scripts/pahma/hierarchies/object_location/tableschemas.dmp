-- Name: get_id(text); Type: FUNCTION; Schema: public; Owner: csadmin
--

CREATE FUNCTION get_id(text) RETURNS character varying
    LANGUAGE plpgsql
    AS $_$
  DECLARE
    -- Declare aliases for user input.
    objID       ALIAS FOR $1;
    objNum_CSID VARCHAR;
  BEGIN
    SELECT INTO objNum_CSID 
    p.pahmaobjectid || '|' || o.objectnumber || '|' || h.name 
      FROM collectionobjects_common o, collectionobjects_pahma p,
           hierarchy h 
       WHERE p.id = o.id and h.id = o.id and p.pahmaobjectid in (objID);
    -- Return the concatenated ID number.
    RETURN objNum_CSID;
  END;
$_$;


