DECLARE
  doc_id varchar(36);
BEGIN
  IF (TG_OP = 'DELETE') THEN
    doc_id := OLD.id;
  ELSE
    doc_id := NEW.id;
  END IF;
  INSERT INTO aclr_modified VALUES(doc_id, 'f');
  RETURN NEW;
END $$;


