-- Name: nx_in_tree(character varying, character varying); Type: FUNCTION; Schema: public; Owner: nuxeo
--

CREATE FUNCTION nx_in_tree(id character varying, baseid character varying) RETURNS boolean
    LANGUAGE plpgsql STABLE COST 400
    AS $$
DECLARE
  curid varchar(36) := id;
BEGIN
  IF baseid IS NULL OR id IS NULL OR baseid = id THEN
    RETURN false;
  END IF;
  LOOP
    SELECT parentid INTO curid FROM hierarchy WHERE hierarchy.id = curid;
    IF curid IS NULL THEN
      RETURN false;
    ELSEIF curid = baseid THEN
      RETURN true;
    END IF;
  END LOOP;
END $$;


