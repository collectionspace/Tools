CREATE FUNCTION nx_ancestors(ids character varying[]) RETURNS SETOF character varying
    LANGUAGE plpgsql
    AS $$
DECLARE
  id varchar(36);
  r record;
BEGIN
  FOR r IN SELECT anc.ancestors FROM ancestors anc
      WHERE anc.id IN (SELECT * FROM unnest(ids)) LOOP
    RETURN QUERY SELECT unnest(r.ancestors);
  END LOOP;
END $$;


