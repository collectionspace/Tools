--
-- Name: createcurrentlocationview(); Type: FUNCTION; Schema: utils; Owner: nuxeo
--

CREATE FUNCTION createcurrentlocationview() RETURNS void
    LANGUAGE sql
    AS $_$
    CREATE OR REPLACE VIEW utils.current_location_view AS
    SELECT
      h2.name AS collectionobjectcsid,
      REGEXP_REPLACE( c.computedcurrentlocation, '^.*\)''(.*)''$', '\1' )
        AS storagelocation,
      (CASE when ca.computedcrate is NULL OR LENGTH(ca.computedcrate) = 0 THEN
         NULL
      ELSE
        REGEXP_REPLACE( ca.computedcrate, '^.*\)''(.*)''$', '\1' )
      END ) as crate
    FROM
      movements_common m
      JOIN hierarchy h1
        ON( m.id = h1.id AND m.currentlocation IS NOT NULL )
      JOIN relations_common r
        ON( h1.name = r.subjectcsid
             AND r.subjectdocumenttype = 'Movement'
             AND r.objectdocumenttype = 'CollectionObject'
             AND h1.primarytype = 'MovementTenant15' )
      JOIN hierarchy h2
        ON( r.objectcsid = h2.name )
      JOIN collectionobjects_common c
        ON( h2.id = c.id )
      JOIN collectionobjects_anthropology ca
        ON( c.id = ca.id )
      JOIN misc misc
        ON( c.id = misc.id AND misc.lifecyclestate <> 'deleted' )
    ORDER BY m.locationdate DESC
$_$;


