--
-- Name: nx_get_local_read_acl(character varying); Type: FUNCTION; Schema: public; Owner: nuxeo
--

CREATE FUNCTION nx_get_local_read_acl(id character varying) RETURNS character varying
    LANGUAGE plpgsql STABLE
    AS $$
-- Compute the read acl for a hierarchy id using a local acl
DECLARE
  curid varchar(36) := id;
  read_acl varchar(4096) := NULL;
  r record;
BEGIN
  -- RAISE DEBUG 'call %', curid;
  FOR r IN SELECT CASE
      WHEN (NOT acls.grant) THEN '-'
      WHEN (acls.grant) THEN ''
      ELSE NULL
    END || acls.user AS op
  FROM acls
  WHERE acls.id = curid AND
      permission IN (SELECT permission FROM aclr_permission)
  ORDER BY acls.pos LOOP
    IF r.op IS NULL THEN
      CONTINUE;
    END IF;
    IF read_acl IS NULL THEN
      read_acl := r.op;
    ELSE
      read_acl := read_acl || ',' || r.op;
    END IF;
  END LOOP;
  RETURN read_acl;
END $$;


ALTER FUNCTION public.nx_get_local_read_acl(id character varying) OWNER TO nuxeo;

--
-- Name: nx_get_read_acl(character varying); Type: FUNCTION; Schema: public; Owner: nuxeo
--

CREATE FUNCTION nx_get_read_acl(id character varying) RETURNS character varying
    LANGUAGE plpgsql STABLE
    AS $$
-- Compute the read acl for a hierarchy id using inherited acl
DECLARE
  curid varchar(36) := id;
  newid varchar(36);
  first boolean := true;
  read_acl varchar(4096);
  ret varchar(4096);
  pos integer;
BEGIN
  WHILE curid IS NOT NULL LOOP
    SELECT nx_get_local_read_acl(curid) INTO read_acl;
    IF read_acl IS NOT NULL THEN
      IF ret is NULL THEN
        ret := read_acl;
      ELSE
        ret := ret || ',' || read_acl;
      END IF;
    END IF;
    SELECT parentid INTO newid FROM hierarchy WHERE hierarchy.id = curid;
    IF first AND newid IS NULL THEN
      SELECT versionableid INTO newid FROM versions WHERE versions.id = curid;
    END IF;
    first := false;
    curid := newid;
    -- Remove everything after a deny on Everyone
    SELECT position('-Everyone' in ret) INTO pos;
    IF pos > 0 THEN
      ret := substring(ret from 1 for pos + length('Everyone'));
      curid := NULL;
    END IF;
  END LOOP;
  IF ret is NULL THEN
    ret = '_empty';
  END IF;
  RETURN ret;
END $$;


ALTER FUNCTION public.nx_get_read_acl(id character varying) OWNER TO nuxeo;

--
-- Name: nx_get_read_acls_for(character varying[]); Type: FUNCTION; Schema: public; Owner: nuxeo
--

CREATE FUNCTION nx_get_read_acls_for(users character varying[]) RETURNS SETOF text
    LANGUAGE plpgsql
    AS $$
-- List read acl ids for a list of user/groups using cache
DECLARE
  in_cache boolean;
  user_md5 varchar(34);
BEGIN
  SELECT md5(array_to_string(users, ',')) INTO user_md5;
  SELECT true INTO in_cache WHERE EXISTS (SELECT 1 FROM aclr_user WHERE user_id = user_md5);
  IF in_cache IS NULL THEN
    INSERT INTO aclr_user VALUES (user_md5, users);
    INSERT INTO aclr_user_map SELECT user_md5, acl_id FROM nx_list_read_acls_for(users) AS acl_id;
  END IF;
  RETURN QUERY SELECT acl_id::text FROM aclr_user_map WHERE user_id = user_md5;
  RETURN;
END $$;


ALTER FUNCTION public.nx_get_read_acls_for(users character varying[]) OWNER TO nuxeo;

