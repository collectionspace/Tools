--
-- Name: current_location_view; Type: VIEW; Schema: utils; Owner: nuxeo
--

CREATE VIEW current_location_view AS
 SELECT h2.name AS collectionobjectcsid,
    regexp_replace((c.computedcurrentlocation)::text, '^.*\)''(.*)''$'::text, '\1'::text) AS storagelocation,
        CASE
            WHEN ((ca.computedcrate IS NULL) OR (length((ca.computedcrate)::text) = 0)) THEN NULL::text
            ELSE regexp_replace((ca.computedcrate)::text, '^.*\)''(.*)''$'::text, '\1'::text)
        END AS crate
   FROM ((((((public.movements_common m
   JOIN public.hierarchy h1 ON ((((m.id)::text = (h1.id)::text) AND (m.currentlocation IS NOT NULL))))
   JOIN public.relations_common r ON ((((((h1.name)::text = (r.subjectcsid)::text) AND ((r.subjectdocumenttype)::text = 'Movement'::text)) AND ((r.objectdocumenttype)::text = 'CollectionObject'::text)) AND ((h1.primarytype)::text = 'MovementTenant15'::text))))
   JOIN public.hierarchy h2 ON (((r.objectcsid)::text = (h2.name)::text)))
   JOIN public.collectionobjects_common c ON (((h2.id)::text = (c.id)::text)))
   JOIN public.collectionobjects_anthropology ca ON (((c.id)::text = (ca.id)::text)))
   JOIN public.misc misc ON ((((c.id)::text = (misc.id)::text) AND ((misc.lifecyclestate)::text <> 'deleted'::text))))
  ORDER BY m.locationdate DESC;


