<div style="float:right;">
#foreach($t in $doc.getFieldValue('blob_txts'))
<a target="image" href="http://ucjeps.cspace.berkeley.edu:8180/cspace-services/blobs/$t/derivatives/OriginalJpeg/content">
<image height="200px" src="http://ucjeps.cspace.berkeley.edu:8180/cspace-services/blobs/$t/derivatives/Thumbnail/content"></a>
#end
</div>
<div>
<div class="result-title"><b>#field('accessionnumber_txt')</b></div>
##do we have a physical location for this specimen
#set($location = $doc.getFieldValue('location'))
#if($location)<div class="map"><img src="http://maps.google.com/maps/api/staticmap?&zoom=12&size=150x80&maptype=roadmap&markers=$doc.getFieldValue('location')&sensor=false" /><div><small><a target="_map" href="http://maps.google.com/?q=$location&amp;source=embed">Larger Map</a></small></div></div>#end
##<div>Id: #field('id')</div>
##<div>Blob CSID: #field('blob_txts')</div>
<div><h2>#field('determination_txt')</h2></div>
<div>#field('rare_txt') /  #field('deadflag_txt') /  #field('dataquality_txt')</div>
<div>#field('gardenlocation_txt')</div>
<div>Collector: #field('collector_txt')</div><div>Locality:
<ul>
#foreach($t in $doc.getFieldValue('locality_txts'))
<li>$t</li>
#end
</ul>
</div>
#set($objectdate = $date.format('MM/dd/yyyy',$doc.getFieldValue('earlycollectiondate_dt')))
#if($doc.getFieldValue('earlycollectiondate_dt'))<div>Collection date: $objectdate</div>#end
#if($doc.getFieldValue('habitat_txt'))<div>Habitat: #field('habitat_txt')</div>#end
#if($doc.getFieldValue('elevation_txt'))<div>Elevation: #field('elevation_txt') #field('elevationunit_txt')</div>#end
#parse('debug.vm')
</div>
